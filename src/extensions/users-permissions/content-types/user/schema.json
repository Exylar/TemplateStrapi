{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Mr",
        "Mme"
      ]
    },
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "phone_number": {
      "type": "string"
    },
    "birthday": {
      "type": "string"
    },
    "image": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::image.image"
    },
    "wallet": {
      "type": "decimal",
      "required": false,
      "min": 0
    },
    "roles": {
      "type": "component",
      "repeatable": true,
      "component": "user.role"
    },
    "event_participation": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "user"
    }
  }
}
